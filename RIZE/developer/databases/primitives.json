{
    "arm":{
        "description":"Perform an stretch",
        "input":"dropdown",
        "input_options":[
            "up",
            "side",
            "forward",
            "crane"
        ],
        "options":{
            "LeftArm":"bool",
            "RightArm":"bool"
        },
        "primitive":"Raise arm",
        "type":"output"
    },
    "leg":{
        "description":"Raise Leg",
        "input":"dropdown",
        "input_options":[
            "forward",
            "backward"
        ],
        "options":{
            "LeftLeg":"bool",
            "RightLeg":"bool",
            "remain_time":"seconds"
        },
        "primitive":"Raise leg",
        "type":"output"
    },
    "mode":{
        "description":"Wake or rest the robot",
        "input":"dropdown",
        "input_options":[
            "wake_up",
            "rest"
        ],
        "primitive":"Mode (Wake up/Rest)",
        "type":"output"
    },
    "reset":{
        "description":"Reset Robot",
        "primitive":"Reset",
        "type":"output"
    },
    "say":{
        "description":"Say something with the robot",
        "input":"string",
        "options":{
            "volume":"percentage"
        },
        "primitive":"Say",
        "type":"output"
    },
    "stretch":{
        "description":"Perform an stretch",
        "input":"dropdown",
        "input_options":[
            "HeadStretch",
            "HeadRoll",
            "EyeRest",
            "WristStretch",
            "ElbowStretch",
            "UpperAndLowerStretch",
            "ShoulderRoll",
            "ForwardStretch",
            "BackArch",
            "Pectoral",
            "SideBend",
            "ChinTuck",
            "FootRotation",
            "HeelRaise"
        ],
        "options":{
            "guide":"bool",
            "repetition":"number",
            "speed":"percentage",
            "volume":"percentage"
        },
        "primitive":"Stretch",
        "type":"output"
    },
    "touched":{
        "description":"Is robot touched",
        "input":"dropdown",
        "input_options":[
            "head",
            "right_hand",
            "left_hand"
        ],
        "primitive":"Touched",
        "type":"input"
    },
    "turn":{
        "description":"Turn robot body/head",
        "input":"dropdown",
        "input_options":[
            "left",
            "right"
        ],
        "options":{
            "body":"bool",
            "degrees":"degrees"
        },
        "primitive":"Turn",
        "type":"output"
    },
    "wait":{
        "description":"Wait some seconds",
        "input":"seconds",
        "primitive":"Wait (seconds)",
        "type":"output"
    },
    "walk":{
        "description":"Make robot walk",
        "input":"dropdown",
        "input_options":[
            "forwards",
            "backwards",
            "left",
            "right"
        ],
        "options":{
            "meters":"meters",
            "speed":"percentage"
        },
        "primitive":"Walk",
        "type":"output"
    }
}